generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model revenue {
  id         String @id @default(uuid())
  name       String
  desciption String
  value      Float
}

model expensives {
  id         String @id @default(uuid())
  name       String
  desciption String
  value      Float
}
