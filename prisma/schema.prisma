generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model revenue {
  id         String @id @default(uuid())
  name       String
  desciption String
  value      Float
}

model expensives {
  id         String @id @default(uuid())
  name       String
  desciption String
  value      Float
}
